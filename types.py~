# -*- coding: utf-8 -*-
my_name = 'John'
print "{} is type {}".format(my_name,type(my_name))

my_int = 57
print "{} is type {}".format(my_int,type(my_int))

my_float = 3.1415926
print "{} is type {}".format(my_float,type(my_float))

my_bool = True
print "{} is type {}".format(my_bool,type(my_bool))

my_none = None
print "{} is type {}".format(my_none,type(my_none))
